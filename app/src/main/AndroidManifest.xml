<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sujitm.cryptocode">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher1"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.sujitm.cryptocode.MainActivity"
           >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.sujitm.cryptocode.AES"
            android:label="Advanced Encryption"
            android:parentActivityName="com.example.sujitm.cryptocode.MainActivity"
            />
        <activity android:name="com.example.sujitm.cryptocode.DES"
            android:label="Data Encryption"
            android:parentActivityName="com.example.sujitm.cryptocode.MainActivity"/>
        <activity android:name="com.example.sujitm.cryptocode.RSA"
            android:label="RS Encryption"
            android:parentActivityName="com.example.sujitm.cryptocode.MainActivity"/>
        <activity android:name="com.example.sujitm.cryptocode.MD5"
            android:label="MD5 Algorithm"
            android:parentActivityName="com.example.sujitm.cryptocode.MainActivity"/>
        <activity android:name="com.example.sujitm.cryptocode.Resetpassword"
            android:label="Set a new password"
            android:parentActivityName="com.example.sujitm.cryptocode.AES"></activity>
    </application>

</manifest>